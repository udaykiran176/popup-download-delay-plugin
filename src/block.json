{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/popup-delay-download",
	"version": "0.1.0",
	"title": "Popup Delay Download",
	"category": "widgets",
	"icon": "download",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"fileUrl": {
			"type": "string",
			"default": ""
		},
		"timerDuration": {
			"type": "number",
			"default": 30
		  },
		  "adsCode": {
			"type": "string",
			"default": ""
		},
		"buttonText": {
			"type": "string",
			"default": "Download"
		},
		"buttonTextColor": {
			"type": "string",
			"default": "#000000"
		},
		"buttonBgColor": {
			"type": "string",
			"default": "#0073aa"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "popup-delay-download",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}